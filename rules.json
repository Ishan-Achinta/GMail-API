{
    "rules": [
      {
        "conditions": [
          {
            "field": "sender",
            "predicate": "contains",
            "value": "noreply@zomato.com"
          },
          {
            "field": "subject",
            "predicate": "contains",
            "value": "Your Zomato order"
          },
          {
            "field": "received_at",
            "predicate": "less_than",
            "value": "2024-02-13"
          }
        ],
        "predicate": "All",
        "actions": [
          {
            "action": "move_message",
            "destination_folder": "SPAM"
          }
        ]
      },
      {
        "conditions": [
          {
            "field": "sender",
            "predicate": "contains",
            "value": "noreply@swiggy.in"
          }
        ],
        "predicate": "Any",
        "actions": [
          {
            "action": "mark_as_read"
          }
        ]
      }
    ]
  }
  